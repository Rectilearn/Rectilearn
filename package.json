{
  "name": "turborepo-basic-shared",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "dev:dev": "turbo run dev --filter=fastapi --filter=web-next",
    "start:dev": "turbo run start --filter=fastapi --filter=web-next"
  },
  "devDependencies": {
    "prettier": "^2.8.8",
    "turbo": "^1.10.14"
  },
  "resolutions": {
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.2"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@6.32.4"
}
